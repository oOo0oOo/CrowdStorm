<figure<%= (not interchange_image.title.nil?) ? ' title="' + interchange_image.title + '"' : ''%>>
	<img src="<%= interchange_image.src %>" alt="<%= interchange_image.alt %>" <% unless interchange_image.sizes.nil? %>
		data-interchange="<% interchange_image.sizes.each do |size| %>[<%= size.src %>, (<%= size.media %>)],<% end %>"<% end %>/>
</figure>
